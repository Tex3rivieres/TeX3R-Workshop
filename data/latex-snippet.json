			"",
			"\\begin{document}",
			"",
			"\\begin{luacode}",
			"\tmesParametres('${5:basique}') --activite/bilan/corrige/cours/DM/DS/flash/interro/TD",
			"\\end{luacode}",
			"\\parametrage",
			"%FIN PARAMÉTRAGE DOCUMENT",
			"",
			"${0:%Le texte du document}",
			"",
			"\\end{document}"
		],
		"description": "Permet de créer un nouveau document (avec paramètres prédéfinis)"
	},

